<?xml version="1.0" encoding="utf-8"?>
<!-- Per Mlie's word and honor, "don't break compatibility with existing saved games" extends so far as "don't create errors in saved games". While I think this is overkill, I respect Mlie's integrity and have incorporated these back into the Mod. -->
<Defs>
  <ThingDef ParentName="BaseFilth">
    <defName>AR_Smithy_Rome</defName>
    <label>stupid Mediterranean smithy (ERROR)</label>
    <category>Filth</category>
    <description>If this is ever encountered in the game as an item, inform Mlie's Github repository's Issues, because it is not meant to be seen.</description>
    <graphicData Inherit="False">
      <texPath>Buildings/Production/TableSmithingRome</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(0.1,0.1)</drawSize>
    </graphicData>
    <altitudeLayer>Filth</altitudeLayer>
    <selectable>true</selectable>
    <statBases>
      <Flammability>0</Flammability>
    </statBases>
    <pathCost>0</pathCost>
    <filth Inherit="False">
      <cleaningSound Inherit="False">Metal_Drop</cleaningSound>
    </filth>
  </ThingDef>
  <ThingDef ParentName="BaseFilth">
    <defName>AR_Smithy_Orient</defName>
    <label>stupid Egyptian smithy (ERROR)</label>
    <category>Filth</category>
    <description>If this is ever encountered in the game as an item, inform Mlie's Github Repository Issues, because it is not meant to be seen.</description>
    <graphicData>
      <texPath>Buildings/Production/TableSmithingOrient</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>Transparent</shaderType>
      <drawSize>(0.1,0.1)</drawSize>
    </graphicData>
    <altitudeLayer>Filth</altitudeLayer>
    <selectable>true</selectable>
    <statBases>
      <Flammability>0</Flammability>
    </statBases>
    <filth Inherit="False">
      <cleaningSound Inherit="False">Metal_Drop</cleaningSound>
    </filth>
  </ThingDef>
</Defs>